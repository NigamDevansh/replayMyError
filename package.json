{
    "name": "error-replay",
    "version": "1.0.0",
    "description": "Lightweight error replay package that records user actions and generates detailed error reports",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.js",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "clean": "rm -rf dist",
        "prepublishOnly": "npm run clean && npm run build"
    },
    "keywords": [
        "error",
        "replay",
        "debugging",
        "tracking",
        "error-reporting",
        "user-actions",
        "react",
        "typescript",
        "error-tracking",
        "session-replay"
    ],
    "author": "devanshnigam",
    "license": "MIT",
    "devDependencies": {
        "typescript": "^5.3.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/NigamDevansh/replayMyError.git"
    },
    "bugs": {
        "url": "https://github.com/NigamDevansh/replayMyError/issues"
    },
    "homepage": "https://github.com/NigamDevansh/replayMyError#readme"
}